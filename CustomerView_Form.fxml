<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane minHeight="650.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CustomerView.CustomerView_FormController">
   <children>
      <Text fill="#000fee" fontSmoothingType="LCD" layoutX="147.0" layoutY="52.0" stroke="#000fee" strokeLineCap="ROUND" strokeLineJoin="ROUND" text="CUSTOMER VIEW">
         <font>
            <Font name="Cambria Bold" size="40.0" />
         </font>
      </Text>
      <Button fx:id="btnshowall" layoutX="246.0" layoutY="226.0" mnemonicParsing="false" onAction="#doshowall" style="-fx-background-color: #000fee;" text="Show All" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnexcel" layoutX="244.0" layoutY="612.0" mnemonicParsing="false" onAction="#doexcel" style="-fx-background-color: #000fee;" text="Export to Excel" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btnarea" layoutX="427.0" layoutY="164.0" mnemonicParsing="false" onAction="#doarea" style="-fx-background-color: #000fee;" text="Filter City" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btncustomer" layoutX="65.0" layoutY="166.0" mnemonicParsing="false" onAction="#docustomer" style="-fx-background-color: #000fee;" text="Filter Customers" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="15.0" />
         </font>
      </Button>
      <TableView fx:id="tblview" layoutX="8.0" layoutY="275.0" prefHeight="327.0" prefWidth="584.0" />
      <Label layoutX="72.0" layoutY="100.0" text="Customer Type">
         <font>
            <Font name="Calibri Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="455.0" layoutY="101.0" text="City">
         <font>
            <Font name="Calibri Bold" size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboarea" editable="true" layoutX="396.0" layoutY="127.0" onAction="#docomboarea" prefWidth="150.0" />
      <ComboBox fx:id="combocustomer" editable="true" layoutX="58.0" layoutY="128.0" onAction="#docombocustomer" prefWidth="150.0" />
   </children>
</AnchorPane>
